{{ header }}
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/@mdi/font@latest/css/materialdesignicons.min.css">
<link href="{{ base }}view/javascript/multi/replacer/css/chunk-vendors.css" rel="preload" as="style">
<link href="{{ base }}view/javascript/multi/replacer/js/app.js" rel="preload" as="script">
<link href="{{ base }}view/javascript/multi/replacer/js/chunk-vendors.js" rel="preload" as="script">
<link href="{{ base }}view/javascript/multi/replacer/css/chunk-vendors.css" rel="stylesheet">
{{ column_left }}
<div id="content" class="multiedit multireplacer">
	<div class="page-header">
		<div class="container-fluid">
			<div class="pull-right">
				<!-- <button type="submit" for="form-replace" data-toggle="tooltip" title="{{ button_save }}" class="btn btn-primary"><i class="fa fa-save"></i></button> -->
				<a href="{{ cancel }}" data-toggle="tooltip" title="{{ button_cancel }}" class="btn btn-default"><i class="fa fa-reply"></i></a></div>
			<h1>{{ heading_title }}</h1>
			<ul class="breadcrumb">
				{% for breadcrumb in breadcrumbs %}
					<li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
				{% endfor %}
			</ul>
		</div>
	</div>
	<div class="container-fluid">
		<div class="panel panel-default">
			<div class="panel-heading">
				<h3 class="panel-title"><i class="fa fa-pencil"></i> {{ breadcrumb_last }}</h3>
			</div>
			<div class="panel-body">
				<div id="data" data-httpserver="{{ base }}"></div>
				<div id="app"></div>
			</div>
		</div>
	</div>
</div>
<script src="{{ base }}view/javascript/multi/replacer/js/chunk-vendors.js"></script>
<script src="{{ base }}view/javascript/multi/replacer/js/app.js"></script>

{{ footer }} 
